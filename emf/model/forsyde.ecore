<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ForSyDe" nsURI="http://forsyde.github.io/ForSyDe.ecore" nsPrefix="">
  <eClassifiers xsi:type="ecore:EClass" name="ForSyDe">
    <eStructuralFeatures xsi:type="ecore:EReference" name="system" eType="#//Application/Application"
        containment="true" resolveProxies="false"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="platform" eType="#//Platform/Platform"
        containment="true" resolveProxies="false"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="extras" eType="#//Extra/Extra"
        containment="true" resolveProxies="false"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="revision" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"
        defaultValueLiteral="1.0.0"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="decisions" eType="#//Common/Graph"
        containment="true"/>
  </eClassifiers>
  <eSubpackages name="Common">
    <eClassifiers xsi:type="ecore:EClass" name="Edge">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="#//Common/IdType"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="innode" eType="#//Common/IdType"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="outnode" eType="#//Common/IdType"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="properties" eType="#//Common/Pair"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Graph">
      <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
          eType="#//Common/Node" containment="true" resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="edges" upperBound="-1"
          eType="#//Common/Edge" containment="true" resolveProxies="false"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EDataType" name="IdType" instanceClassName="java.lang.String"/>
    <eClassifiers xsi:type="ecore:EClass" name="Node" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="definition" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="properties" upperBound="-1"
          eType="#//Common/Pair" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Element">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="#//Common/IdType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Pair">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="key" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Table" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="items" upperBound="-1"
          eType="#//Common/TableItem" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="names" unique="false"
          lowerBound="1" upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TableItem">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="values" unique="false"
          lowerBound="1" upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Application">
    <eClassifiers xsi:type="ecore:EClass" name="Process" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="constructor" eType="#//Common/IdType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Signal" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="dataType" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Application" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="netlist" ordered="false"
          lowerBound="1" eType="#//Common/Graph" containment="true" resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="processes" ordered="false"
          upperBound="-1" eType="#//Application/Process" volatile="true" derived="true"
          containment="true" resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="functions" ordered="false"
          upperBound="-1" eType="#//Application/Function" volatile="true" derived="true"
          containment="true" resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="constructors" ordered="false"
          upperBound="-1" eType="#//Application/Constructor" volatile="true" derived="true"
          containment="true" resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="signals" ordered="false"
          upperBound="-1" eType="#//Application/Signal" volatile="true" derived="true"
          containment="true" resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="subsystems" ordered="false"
          upperBound="-1" eType="#//Application/Application" derived="true" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="exportedSignals" upperBound="-1"
          eType="#//Common/IdType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Constructor" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="functions" upperBound="-1"
          eType="#//Common/Pair" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="values" upperBound="-1"
          eType="#//Common/Pair" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="implementations" upperBound="-1"
          eType="#//Application/Implementation"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Function" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="inputs" upperBound="-1"
          eType="#//Common/Pair" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="outputs" upperBound="-1"
          eType="#//Common/Pair" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="implementations" upperBound="-1"
          eType="#//Application/Implementation" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Implementation" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="target" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="source" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Platform">
    <eClassifiers xsi:type="ecore:EClass" name="Communication" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameter" upperBound="-1"
          eType="#//Common/Pair" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Computation" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameter" upperBound="-1"
          eType="#//Common/Pair" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Platform" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="computators" upperBound="-1"
          eType="#//Platform/Computation" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="communicators" upperBound="-1"
          eType="#//Platform/Communication" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="netlist" eType="#//Common/Graph"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="subplatforms" upperBound="-1"
          eType="#//Platform/Platform" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="storages" upperBound="-1"
          eType="#//Platform/Storage" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="exportedCommunicators"
          upperBound="-1" eType="#//Common/IdType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Storage" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameter" upperBound="-1"
          eType="#//Common/Pair" volatile="true" derived="true" containment="true"
          resolveProxies="false"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="Extra">
    <eClassifiers xsi:type="ecore:EClass" name="Extra">
      <eStructuralFeatures xsi:type="ecore:EReference" name="information" upperBound="-1"
          eType="#//Common/Table" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Goal" eSuperTypes="#//Common/Element">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="nature" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="process" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="target" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
