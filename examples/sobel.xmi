<?xml version="1.0" encoding="UTF-8"?>
<ForSyDe.Model.IO:ForSyDeIO
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ForSyDe.Model.Application="https://forsyde.github.io/ForSyDe/Model/Application"
    xmlns:ForSyDe.Model.IO="https://forsyde.github.io/ForSyDe/IO"
    xmlns:ForSyDe.Model.Platform="https://forsyde.github.io/ForSyDe/Model/Platform"
    xmlns:ForSyDe.Model.Refinement="https://forsyde.github.io/ForSyDe/Model/Refinement"
    xsi:schemaLocation="https://forsyde.github.io/ForSyDe/Model/Application ../model/forsyde.ecore#//Model/Application https://forsyde.github.io/ForSyDe/IO ../model/forsyde.ecore#//Model/IO https://forsyde.github.io/ForSyDe/Model/Platform ../model/forsyde.ecore#//Model/Platform https://forsyde.github.io/ForSyDe/Model/Refinement ../model/forsyde.ecore#//Model/Refinement">
  <applications
      xsi:type="ForSyDe.Model.Application:CompoundProcess"
      identifier="sobelTop">
    <inPorts
        identifier="sobelIn"
        definer="imgIn"/>
    <outPorts
        identifier="sobelOut"/>
    <vertexes
        xsi:type="ForSyDe.Model.Application:ConstructedProcess"
        identifier="getPixelProc">
      <inPorts
          identifier="imgIn"
          definer="imgInC"/>
      <outPorts
          identifier="pixelOut"
          definer="pixelOutC"/>
      <constructor
          xsi:type="ForSyDe.Model.Application:SDFComb"
          identifier="getPixelCons"
          actor="getPixel">
        <inPorts
            identifier="imgInC"
            definer="img"/>
        <outPorts
            identifier="pixelOutC"
            definer="pixel"/>
      </constructor>
    </vertexes>
    <vertexes
        xsi:type="ForSyDe.Model.Application:Signal"
        identifier="pixelXSig"/>
    <vertexes
        xsi:type="ForSyDe.Model.Application:Signal"
        identifier="pixelYSig"/>
    <vertexes
        xsi:type="ForSyDe.Model.Application:ConstructedProcess"
        identifier="GxP">
      <constructor
          xsi:type="ForSyDe.Model.Application:SDFComb"
          identifier="GxC"
          actor="Gx"/>
    </vertexes>
    <vertexes
        xsi:type="ForSyDe.Model.Application:ConstructedProcess"
        identifier="GyP">
      <constructor
          xsi:type="ForSyDe.Model.Application:SDFComb"
          identifier="GyC"
          actor="Gy"/>
    </vertexes>
    <vertexes
        xsi:type="ForSyDe.Model.Application:ConstructedProcess"
        identifier="absP">
      <constructor
          xsi:type="ForSyDe.Model.Application:SDFComb"
          identifier="absC"
          actor="abs"/>
    </vertexes>
    <vertexes
        xsi:type="ForSyDe.Model.Application:Signal"
        identifier="gxSig"/>
    <vertexes
        xsi:type="ForSyDe.Model.Application:Signal"
        identifier="gySig"/>
    <edges identifier="pToX"
        fromVertex="getPixelProc"
        toVertex="pixelXSig"/>
    <edges identifier="pToY"
        fromVertex="getPixelProc"
        toVertex="pixelYSig"/>
    <edges identifier="XSigtoG"
        fromVertex="pixelXSig"
        toVertex="GxP"/>
    <edges identifier="YSigToG"
        fromVertex="pixelYSig"
        toVertex="GyP"/>
    <edges identifier="GtoX"
        fromVertex="GxP"
        toVertex="gxSig"/>
    <edges identifier="GtoY"
        fromVertex="GyP"
        toVertex="gySig"/>
    <edges identifier="XtoAbs"
        fromVertex="gxSig"
        toVertex="absP"/>
    <edges identifier="YtoAbs"
        fromVertex="gySig"
        toVertex="absP"/>
  </applications>
  <platforms
      xsi:type="ForSyDe.Model.Platform:Structure"
      identifier="MPSoC">
    <customReferences
        xsi:type="ForSyDe.Model.Platform:Computation"
        identifier="cpu1"/>
    <customReferences
        xsi:type="ForSyDe.Model.Platform:Computation"
        identifier="cpu2"/>
    <customReferences
        xsi:type="ForSyDe.Model.Platform:Computation"
        identifier="cpu3"/>
    <vertexes
        xsi:type="ForSyDe.Model.Platform:Computation"
        identifier="cpu4"/>
    <vertexes
        xsi:type="ForSyDe.Model.Platform:TimeDivisionMultiplexer"
        identifier="TDMA"
        maxSlots="4"
        minSlots="4"/>
  </platforms>
  <elements
      xsi:type="ForSyDe.Model.Application:Function"
      identifier="getPixel">
    <inputs
        identifier="img"
        definer="pixelT"/>
    <outputs
        identifier="pixel"
        definer="pixelT"/>
  </elements>
  <elements
      xsi:type="ForSyDe.Model.Application:Function"
      identifier="Gx"/>
  <elements
      xsi:type="ForSyDe.Model.Application:Function"
      identifier="Gy"/>
  <elements
      xsi:type="ForSyDe.Model.Application:Function"
      identifier="abs"/>
  <elements
      xsi:type="ForSyDe.Model.Application:DataType"
      identifier="pixelT"
      sizeValue="1"
      category="Integer"/>
  <maleableRefinement
      xsi:type="ForSyDe.Model.Refinement:StaticCyclicSchedule"
      identifier="orderingV"/>
</ForSyDe.Model.IO:ForSyDeIO>
