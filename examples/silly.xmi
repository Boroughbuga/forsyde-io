<?xml version="1.0" encoding="UTF-8"?>
<ForSyDe.Model.IO:ForSyDeIO
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ForSyDe.Model.Application="https://forsyde.github.io/ForSyDe/Model/Application"
    xmlns:ForSyDe.Model.IO="https://forsyde.github.io/ForSyDe/IO"
    xsi:schemaLocation="https://forsyde.github.io/ForSyDe/Model/Application ../model/forsyde.ecore#//Model/Application https://forsyde.github.io/ForSyDe/IO ../model/forsyde.ecore#//Model/IO">
  <applications
      xsi:type="ForSyDe.Model.Application:CompoundProcess"
      identifier="sillyTotal">
    <inPorts
        identifier="sillyIn"
        definer="bool2IntPIn"/>
    <outPorts
        identifier="sillyOut"
        definer="mealySYOut"/>
    <vertexes
        xsi:type="ForSyDe.Model.Application:ConstructedProcess"
        identifier="bool2intSY">
      <inPorts
          identifier="bool2IntPIn"
          definer="bool2IntCIn"/>
      <outPorts
          identifier="bool2IntPOut"
          definer="bool2IntCOut"/>
      <constructor
          xsi:type="ForSyDe.Model.Application:SYComb"
          identifier="con1"
          function="bool2int">
        <inPorts
            identifier="bool2IntCIn"
            definer="boolIn"/>
        <outPorts
            identifier="bool2IntCOut"
            definer="intOut"/>
      </constructor>
    </vertexes>
    <vertexes
        xsi:type="ForSyDe.Model.Application:CompoundProcess"
        identifier="mealySY">
      <inPorts
          identifier="mealySYin"
          definer="statePIn"/>
      <inPorts
          identifier="mealySYOut"
          definer="outOutP"/>
      <vertexes
          xsi:type="ForSyDe.Model.Application:ConstructedProcess"
          identifier="stateSY">
        <inPorts
            identifier="statePCur"
            definer="statePCCur"/>
        <inPorts
            identifier="statePIn"
            definer="statePCIn"/>
        <outPorts
            identifier="statePNext"
            definer="statePCNext"/>
        <constructor
            xsi:type="ForSyDe.Model.Application:SYComb"
            identifier="statePC">
          <inPorts
              identifier="statePCCur"
              definer="curState"/>
          <inPorts
              identifier="statePCIn"
              definer="stateIn"/>
          <outPorts
              identifier="statePCNext"
              definer="nextState"/>
        </constructor>
      </vertexes>
      <vertexes
          xsi:type="ForSyDe.Model.Application:ConstructedProcess"
          identifier="outputSY">
        <inPorts
            identifier="outInP"
            definer="outInC"/>
        <outPorts
            identifier="outOutP"
            definer="outOutC"/>
        <constructor
            xsi:type="ForSyDe.Model.Application:SYComb"
            identifier="outPC"
            function="outputFunc">
          <inPorts
              identifier="outInC"
              definer="outIn"/>
          <outPorts
              identifier="outOutC"
              definer="outOut"/>
        </constructor>
      </vertexes>
      <vertexes
          xsi:type="ForSyDe.Model.Application:ConstructedProcess"
          identifier="delaySY">
        <inPorts
            identifier="dIn"
            definer="dInC"/>
        <outPorts
            identifier="dOut"
            definer="dOutC"/>
        <constructor
            xsi:type="ForSyDe.Model.Application:SYDelay"
            identifier="delaySYCon"
            value="0">
          <inPorts
              identifier="dInC"/>
          <outPorts
              identifier="dOutC"/>
        </constructor>
      </vertexes>
      <vertexes
          xsi:type="ForSyDe.Model.Application:Signal"
          identifier="nextStateSig"/>
      <vertexes
          xsi:type="ForSyDe.Model.Application:Signal"
          identifier="nextToDelaySig"/>
    </vertexes>
    <vertexes
        xsi:type="ForSyDe.Model.Application:Signal"
        identifier="convertedSig"/>
  </applications>
  <elements
      xsi:type="ForSyDe.Model.Application:Function"
      identifier="bool2int">
    <inputs
        identifier="boolIn"
        definer="Bool"/>
    <outputs
        identifier="intOut"
        definer="Int8"/>
  </elements>
  <elements
      xsi:type="ForSyDe.Model.Application:Function"
      identifier="stateFunc">
    <inputs
        identifier="curState"
        definer="Int8"/>
    <inputs
        identifier="stateIn"
        definer="Int8"/>
    <outputs
        identifier="nextState"
        definer="Int8"/>
  </elements>
  <elements
      xsi:type="ForSyDe.Model.Application:Function"
      identifier="outputFunc">
    <inputs
        identifier="outIn"
        definer="Int8"/>
    <outputs
        identifier="outOut"
        definer="Int8"/>
  </elements>
  <elements
      xsi:type="ForSyDe.Model.Application:DataType"
      identifier="Int8"
      sizeValue="1"
      category="Integer"/>
  <elements
      xsi:type="ForSyDe.Model.Application:DataType"
      identifier="Bool"
      sizeValue="1"
      sizeUnit="&quot;b&quot;"
      category="Bit"/>
</ForSyDe.Model.IO:ForSyDeIO>
