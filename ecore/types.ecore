<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Types" nsURI="https://forsyde.github.io/Types" nsPrefix="Types">
  <eSubpackages name="Application" nsURI="https://forsyde.github.io/ForSyDe/Model/Application"
      nsPrefix="ForSyDe.Model.Application">
    <eClassifiers xsi:type="ecore:EClass" name="DataType"/>
    <eClassifiers xsi:type="ecore:EClass" name="ConstructedProcess" eSuperTypes="#//Application/Process"/>
    <eClassifiers xsi:type="ecore:EClass" name="Signal"/>
    <eClassifiers xsi:type="ecore:EClass" name="NativeFunction"/>
    <eClassifiers xsi:type="ecore:EClass" name="Implementation"/>
    <eClassifiers xsi:type="ecore:EClass" name="SDFComb"/>
    <eClassifiers xsi:type="ecore:EClass" name="SDFPrefix"/>
    <eClassifiers xsi:type="ecore:EClass" name="SYComb"/>
    <eClassifiers xsi:type="ecore:EClass" name="SYPrefix"/>
    <eClassifiers xsi:type="ecore:EClass" name="DEPrefix"/>
    <eClassifiers xsi:type="ecore:EClass" name="DEValueComb"/>
    <eClassifiers xsi:type="ecore:EClass" name="DETagComb"/>
    <eClassifiers xsi:type="ecore:EClass" name="Implements"/>
    <eClassifiers xsi:type="ecore:EClass" name="Constructs"/>
    <eClassifiers xsi:type="ecore:EClass" name="Types"/>
    <eClassifiers xsi:type="ecore:EClass" name="Takes"/>
    <eClassifiers xsi:type="ecore:EClass" name="Writes"/>
    <eClassifiers xsi:type="ecore:EClass" name="Reads"/>
    <eClassifiers xsi:type="ecore:EClass" name="Process"/>
    <eClassifiers xsi:type="ecore:EClass" name="FIFOSignal" eSuperTypes="#//Application/Signal"/>
    <eClassifiers xsi:type="ecore:EClass" name="Input"/>
    <eClassifiers xsi:type="ecore:EClass" name="Output"/>
    <eClassifiers xsi:type="ecore:EClass" name="Description"/>
  </eSubpackages>
  <eSubpackages name="Platform" nsURI="https://forsyde.github.io/ForSyDe/Model/Platform"
      nsPrefix="ForSyDe.Model.Platform">
    <eClassifiers xsi:type="ecore:EClass" name="Communication"/>
    <eClassifiers xsi:type="ecore:EClass" name="Computation"/>
    <eClassifiers xsi:type="ecore:EClass" name="Storage"/>
    <eClassifiers xsi:type="ecore:EClass" name="Interface"/>
    <eClassifiers xsi:type="ecore:EClass" name="Structure"/>
    <eClassifiers xsi:type="ecore:EClass" name="TimeDivisionMultiplexer" eSuperTypes="#//Platform/Communication">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxSlots" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Integer"
          defaultValueLiteral="1"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minSlots" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Integer"
          defaultValueLiteral="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ComputationalTile" eSuperTypes="#//Platform/Computation"/>
  </eSubpackages>
  <eSubpackages name="Extra" nsURI="https://forsyde.github.io/ForSyDe/Model/Extra"
      nsPrefix="ForSyDe.Model.Extra">
    <eClassifiers xsi:type="ecore:EClass" name="Goal"/>
    <eClassifiers xsi:type="ecore:EClass" name="Throughput" eSuperTypes="#//Extra/Goal"/>
  </eSubpackages>
  <eSubpackages name="Refinement" nsURI="https://forsyde.github.io/ForSyDe/Model/Refinement"
      nsPrefix="ForSyDe.Model.Refinement">
    <eClassifiers xsi:type="ecore:EClass" name="SporadicTask">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minArrival" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Integer"
          defaultValueLiteral="0"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TriggeredTask"/>
    <eClassifiers xsi:type="ecore:EClass" name="StaticCyclicSchedule"/>
    <eClassifiers xsi:type="ecore:EClass" name="FixedPriorityScheduler">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="preemptive" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Boolean"
          defaultValueLiteral="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CustomScheduler"/>
    <eClassifiers xsi:type="ecore:EClass" name="RoundRobinScheduler"/>
  </eSubpackages>
</ecore:EPackage>
